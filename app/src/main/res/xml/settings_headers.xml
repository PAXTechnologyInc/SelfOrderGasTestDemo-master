<preference-headers xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 商户 -->
    <header
        android:fragment="com.pax.order.settings.SettingTableNumFragment"
        android:icon="@drawable/app_setting"
        android:title="@string/settings_menu_merchant_parameter" />

    <!-- stand-by time  -->
    <header
        android:fragment="com.pax.order.settings.SettingsStandByParamFragment"
        android:icon="@drawable/app_setting"
        android:title="@string/settings_menu_standby_parameter" />

    <!-- 支付方式设置 -->
    <header
        android:fragment="com.pax.order.settings.PayModuleSettingFragment"
        android:icon="@drawable/app_setting"
        android:title="@string/pay_setting" />
    <!-- 打印相关设置 -->
    <header
        android:fragment="com.pax.order.settings.PrintConfigSettingFragment"
        android:icon="@drawable/app_setting"
        android:title="@string/prn_config" />
    <!-- Bluetook configuration -->
    <header
        android:fragment="com.pax.order.settings.BluetoothSettingFragment"
        android:icon="@drawable/app_setting"
        android:title="@string/bluetooth_setting" />

    <!-- 操作员改密 -->
