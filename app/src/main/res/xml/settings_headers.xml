    <header
        android:fragment="com.pax.order.settings.VersionFragment"
        android:icon="@drawable/app_version"
        android:title="@string/settings_version" />

    <!-- 退出 -->
    <header
        android:fragment="com.pax.order.settings.ExitFragment"
        android:icon="@drawable/app_logout"
        android:title="@string/settings_exit" />
</preference-headers>