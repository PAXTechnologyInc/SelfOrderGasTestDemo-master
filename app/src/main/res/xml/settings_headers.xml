        android:fragment="com.pax.order.settings.OperatorModPWDrFragment"
        android:icon="@drawable/app_modifypwd"
        android:title="@string/modify_oper_pwd" />

    <!-- 结算 -->
    <header
        android:fragment="com.pax.order.settings.BatchFragment"
